generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    name String
    avatar String
    eMail String
}

model Card {
  id    Int    @id @default(autoincrement())
  front String
  back  String
}
